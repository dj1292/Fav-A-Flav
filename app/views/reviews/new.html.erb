<%= form_for @review, method: :post do |f| %>
<%= f.label :title %>
<%= f.text_field :title %> <br>
<%= f.label :content %>
<%= f.text_area :content %> <br>
<%= f.collection_select(:user_id, User.all, :id, :username) %>
<%# <%= f.collection_select(:plate_id, Plate.all, :id, :name) %> %>
<%= f.submit %>
<% end %>